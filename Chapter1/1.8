using namespace std;

const char* message(int n);


int main()
{


	message(3);
	return 0;

}
const char* message(int n)
{
	const int combination = 3;
	const char * sentence[combination] = { "good","pass","fail" };
	if (n < 0)
	{
		n = 0;
	}
	else if (n >= combination)
	{
		n = n - 1;
	}

	return sentence[n];
}
